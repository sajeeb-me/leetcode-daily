problem: https://leetcode.com/problems/check-if-a-string-contains-all-binary-codes-of-size-k/description/?envType=daily-question&envId=2026-02-23

solution:
class Solution {
public:
    bool hasAllCodes(string s, int k) {
        if(k > s.length()) return false;
        unordered_set<string>st;
        
        for(int i=0; i<=s.length()-k; i++){
            st.insert(s.substr(i, k));
        }

        return st.size() == pow(2, k);
    }
};
