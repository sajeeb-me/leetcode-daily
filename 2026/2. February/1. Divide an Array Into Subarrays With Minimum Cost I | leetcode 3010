problem: https://leetcode.com/problems/divide-an-array-into-subarrays-with-minimum-cost-i/description/?envType=daily-question&envId=2026-02-01

solution:
class Solution {
public:
    int minimumCost(vector<int>& nums) {
        int m1=100, m2=100;
        for(int i=1; i<nums.size(); i++){
            if(nums[i] < m1){
                m2 = m1;
                m1 = nums[i];
            } else if(nums[i] < m2){
                m2 = nums[i];
            }
        }
        return nums[0] + m1 + m2;
    }
};
