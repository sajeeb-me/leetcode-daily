problem: https://leetcode.com/problems/transformed-array/description/?envType=daily-question&envId=2026-02-05

solution:
class Solution {
public:
    vector<int> constructTransformedArray(vector<int>& nums) {
        int n = nums.size();
        vector<int> res;

        for (int i = 0; i < n; i++){
            res.push_back(nums[(((i + nums[i]) % n) + n) % n]);
        }

        return res;
    }
};
