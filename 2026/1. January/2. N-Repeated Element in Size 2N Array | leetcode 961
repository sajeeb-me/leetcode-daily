};

class Solution {
public:
    int repeatedNTimes(vector<int>& nums) {
        int res = 0, currFreq = 0;
        unordered_map<int, int> freq;
        for(int num : nums){
            freq[num]++;
            if(freq[num] > currFreq){
                currFreq = freq[num];
                res = num;
            }
        }
        return res;
    }
};
