problem: https://leetcode.com/problems/convert-integer-to-the-sum-of-two-no-zero-integers/description/?envType=daily-question&envId=2025-09-08

solution:
class Solution {
private:
    bool containZero(int n){
        string str = to_string(n);
        return str.find('0') != string::npos;
    }

public:
    vector<int> getNoZeroIntegers(int n) {
        int firstNum, secondNum;
        for(int i=1; i<n; i++){
            firstNum = i; secondNum = n-i;
            if(!containZero(firstNum) && !containZero(secondNum))
                return {firstNum, secondNum};
        }
        return {};
    }
};
