problem: https://leetcode.com/problems/count-the-number-of-computer-unlocking-permutations/description/?envType=daily-question&envId=2025-12-10

solution:
class Solution {
public:
    int countPermutations(vector<int>& complexity) {
        int MOD = 1e9+7;
        int res = 1;
        for(int i=1; i<complexity.size(); i++){
            if(complexity[i] <= complexity[0]) return 0;
            res = (1LL * res * i) % MOD;
        }
        return res;
    }
};
